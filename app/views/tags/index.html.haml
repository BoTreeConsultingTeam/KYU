= render :partial => "layouts/sub_heading", :locals => {:object => 'Tags'}
- if (administrator_signed_in?)
  = link_to 'Add tag', new_tag_path
.main-tag-list
  .tags-list
    .wrap-tag-list
      .clearfix
      .question-cat
        %ul.question-tags
          - @tags.each do |tag|
            = render :partial => "tag_links", :locals => {:tag => tag}
.clearfix
  .row.paginations.home
    .col-md-12
      =paginate @tags  
    
