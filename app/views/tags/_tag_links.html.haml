%li
= link_to tag, questions_path(tag: tag.name),class: 'q-tag'
- if (administrator_signed_in?)
  = link_to '',edit_tag_path(tag.id),'data-no-turbolink' => true, class: 'fa fa-pencil'
  |
  = link_to '',delete_tag_path(tag.id), method: :delete, data: {confirm: 'Are you sure?'}, class: 'fa fa-trash-o'