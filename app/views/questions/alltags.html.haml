/ -@tags.each do |tag|
/ 	= link_to tag, tag_path(tag.name)
/ 	%br 
.container-fluid
  .row
    / END HEADER
    #header_sidebar.col-md-12.col-xs-12
    = render 'layouts/left_sidebar'
    .col-md-8.main-content
      = render :partial => "layouts/sub_heading", :locals => {:object => 'Tags'}
      .main-tag-list
        .tags-list
          .wrap-tag-list
            .clearfix
            .question-cat
              %ul.question-tags
                - @tags.each do |tag|
                  %li
                    = link_to tag, tag_path(tag.name),class: 'q-tag'   
      .clearfix
        .row.paginations.home
          .col-md-12
            =paginate @tags  
    = render 'layouts/right_sidebar'
  / END ROW